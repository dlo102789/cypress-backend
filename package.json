{
  "name": "cypress-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "build": "tsc && eslint src",
    "serve": "node dist/app.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "npm run build && npm run watch-debug"
  },
  "dependencies": {
    "aws-sdk": "^2.1156.0",
    "body-parser": "^1.20.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "sequelize": "^6.21.0",
    "ts-node": "^10.8.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "mocha": "^10.0.0",
    "typescript": "^4.7.3"
  }
}
